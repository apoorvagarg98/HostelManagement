<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".matapitaprofileView">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="400dp"
        android:layout_height="400dp"
        android:layout_marginStart="3dp"
        android:layout_marginLeft="3dp"
        android:layout_marginEnd="3dp"
        android:layout_marginRight="3dp"
        android:visibility="visible"
        app:cardBackgroundColor="#C0CDD8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.31"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.32999998">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="397dp"
            android:layout_height="match_parent">

            <EditText
                android:id="@+id/exitDate2"
                android:layout_width="350dp"
                android:layout_height="47dp"
                android:layout_marginBottom="4dp"
                android:background="@drawable/inpuut_bg"
                android:drawableLeft="@drawable/ic_baseline_calendar_today_2421"
                android:drawablePadding="5dp"
                android:ems="10"
                android:hint="Enter Exit Date"
                android:inputType="date"
                android:padding="10dp"
                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:textColorHint="#090909"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/exitTime2"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/exitTime2" />

            <EditText
                android:id="@+id/exitTime2"
                android:layout_width="350dp"
                android:layout_height="47dp"

                android:layout_marginBottom="12dp"
                android:background="@drawable/inpuut_bg"
                android:drawableLeft="@drawable/ic_time"
                android:drawablePadding="5dp"
                android:ems="10"
                android:hint=" Enter Exit Time"
                android:inputType="time"
                android:padding="10dp"
                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:textColorHint="#0E0E0E"
                app:layout_constraintBottom_toTopOf="@+id/exitDate2"
                app:layout_constraintEnd_toEndOf="@+id/entryDate2"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/entryDate2" />

            <EditText
                android:id="@+id/entryTime2"
                android:layout_width="350dp"
                android:layout_height="47dp"
                android:layout_marginBottom="12dp"
                android:background="@drawable/inpuut_bg"
                android:drawableLeft="@drawable/ic_baseline_access_time_24"
                android:drawablePadding="5dp"
                android:ems="10"
                android:hint=" Enter Entry Time"
                android:inputType="time"
                android:padding="10dp"

                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:textColorHint="#151515"
                app:layout_constraintBottom_toTopOf="@+id/entryDate2"
                app:layout_constraintEnd_toEndOf="@+id/adress2"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/adress2" />

            <EditText
                android:id="@+id/entryDate2"
                android:layout_width="350dp"
                android:layout_height="47dp"
                android:layout_marginBottom="12dp"
                android:background="@drawable/inpuut_bg"
                android:drawableLeft="@drawable/ic_baseline_calendar_today_24"
                android:drawablePadding="5dp"

                android:ems="10"
                android:hint="Enter Entry date"
                android:inputType="date"
                android:padding="10dp"
                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:textColorHint="#101010"
                app:layout_constraintBottom_toTopOf="@+id/exitTime2"
                app:layout_constraintEnd_toEndOf="@+id/entryTime2"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/entryTime2" />

            <EditText
                android:id="@+id/adress2"
                android:layout_width="350dp"
                android:layout_height="47dp"
                android:background="@drawable/inpuut_bg"
                android:drawableLeft="@drawable/ic_baseline_add_location_24"
                android:drawablePadding="5dp"
                android:ems="10"
                android:hint=" Address "
                android:inputType="textPostalAddress"
                android:padding="10dp"
                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:textColorHint="#0C0C0C"
                app:layout_constraintBottom_toTopOf="@+id/entryTime2"
                app:layout_constraintEnd_toEndOf="@+id/nameinstudentutpass"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/nameinstudentutpass"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.93" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="346dp"
                android:layout_height="52dp"
                android:layout_marginTop="8dp"
                android:text=" Student OutPass"
                android:textAllCaps="true"
                android:textColor="#420707"
                android:textSize="34sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/nameinstudentutpass"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.588"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="1.0" />

            <EditText
                android:id="@+id/nameinstudentutpass"
                android:layout_width="350dp"
                android:layout_height="47dp"
                android:layout_marginBottom="8dp"
                android:background="@drawable/inpuut_bg"
                android:drawableLeft="@drawable/ic_baseline_text_fields_24"
                android:drawablePadding="5dp"
                android:ems="10"
                android:hint="Name"
                android:inputType="textPersonName"
                android:paddingLeft="10dp"
                android:textColorHint="#101010"
                app:layout_constraintBottom_toTopOf="@+id/adress2"
                app:layout_constraintEnd_toEndOf="@+id/textView3"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@+id/textView3"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="1.0" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/usernameparentsshow"
        android:layout_width="282dp"
        android:layout_height="40dp"
        android:text="Name:"
        android:textColor="#070707"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/cardView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.086" />

    <TextView
        android:id="@+id/showPhone"
        android:layout_width="278dp"
        android:layout_height="31dp"
        android:text="Phone No:"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/cardView"
        app:layout_constraintEnd_toEndOf="@+id/usernameparentsshow"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/usernameparentsshow"
        app:layout_constraintTop_toBottomOf="@+id/usernameparentsshow"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/request9810"
        android:layout_width="176dp"
        android:layout_height="53dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="128dp"
        android:background="@drawable/btnn_bg"
        android:text="request"
        app:backgroundTint="@color/design_default_color_primary_dark"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/cancelRequest"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/list" />

    <ListView
        android:id="@+id/list"
        android:layout_width="395dp"
        android:layout_height="286dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.53">

    </ListView>

    <TextView
        android:id="@+id/showEmail"
        android:layout_width="270dp"
        android:layout_height="26dp"
        android:layout_marginTop="1dp"
        android:text="Email:"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/cardView"
        app:layout_constraintEnd_toEndOf="@+id/showPhone"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/showPhone"
        app:layout_constraintTop_toBottomOf="@+id/showPhone"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="100dp"
        android:layout_height="114dp"
        android:layout_marginTop="16dp"
        app:layout_constraintBottom_toTopOf="@+id/cardView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.583"
        app:layout_constraintStart_toEndOf="@+id/usernameparentsshow"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/jgiicon" />

    <Button
        android:id="@+id/pushreqtowarden"
        android:layout_width="160dp"
        android:layout_height="49dp"
        android:background="@drawable/btnn_bg"
        android:text="REQUEST TO WARDEN"
        android:visibility="invisible"
        app:backgroundTint="@android:color/holo_orange_dark"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/ViewbacchokiHistory"
        app:layout_constraintHorizontal_bias="0.553"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/request9810"
        app:layout_constraintVertical_bias="0.37" />

    <Button
        android:id="@+id/ViewbacchokiHistory"
        android:layout_width="158dp"
        android:layout_height="53dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/btnn_bg"
        android:text="History"
        app:backgroundTint="@color/design_default_color_primary"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.83"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cancelRequest"
        app:layout_constraintVertical_bias="0.087" />

    <Button
        android:id="@+id/cancelRequest"
        android:layout_width="176dp"
        android:layout_height="53dp"
        android:background="@drawable/btnn_bg"
        android:text="cancel Request"
        app:layout_constraintBottom_toBottomOf="@+id/request9810"
        app:layout_constraintEnd_toEndOf="@+id/ViewbacchokiHistory"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ViewbacchokiHistory"
        app:layout_constraintTop_toTopOf="@+id/request9810"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>