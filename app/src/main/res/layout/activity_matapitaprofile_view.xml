<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".matapitaprofileView">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="420dp"
        android:layout_height="419dp"
        android:layout_marginStart="3dp"
        android:layout_marginLeft="3dp"
        android:layout_marginEnd="3dp"
        android:layout_marginRight="3dp"
        android:visibility="visible"
        app:cardBackgroundColor="#C0CDD8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.31"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.32999998">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="397dp"
            android:layout_height="match_parent">

            <EditText
                android:id="@+id/exitDate2"
                android:layout_width="350dp"
                android:layout_height="47dp"
                android:layout_marginTop="116dp"
                android:layout_marginBottom="44dp"
                android:background="@drawable/inpuut_bg"
                android:drawableLeft="@android:drawable/ic_menu_month"
                android:ems="10"
                android:hint="Enter Exit Date"
                android:inputType="date"
                android:padding="10dp"
                android:paddingLeft="20dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:textColorHint="#090909"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/entryTime2"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/entryTime2"
                app:layout_constraintTop_toBottomOf="@+id/entryTime2" />

            <EditText
                android:id="@+id/exitTime2"
                android:layout_width="350dp"
                android:layout_height="47dp"

                android:background="@drawable/inpuut_bg"
                android:drawableLeft="@drawable/ic_time"
                android:drawablePadding="10dp"
                android:ems="10"
                android:hint=" Enter exit time"
                android:inputType="time"
                android:padding="10dp"
                android:paddingLeft="20dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:textColorHint="#0E0E0E"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/entryDate2"
                app:layout_constraintHorizontal_bias="0.592"
                app:layout_constraintStart_toStartOf="@+id/entryDate2"
                app:layout_constraintTop_toBottomOf="@+id/exitDate2"
                app:layout_constraintVertical_bias="1.0" />

            <EditText
                android:id="@+id/entryTime2"
                android:layout_width="350dp"
                android:layout_height="47dp"
                android:layout_marginTop="12dp"
                android:background="@drawable/inpuut_bg"
                android:drawableLeft="@android:drawable/ic_lock_idle_alarm"
                android:ems="10"
                android:hint=" Enter Entry Time"
                android:inputType="time"
                android:padding="10dp"
                android:paddingLeft="20dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:textColorHint="#151515"
                app:layout_constraintBottom_toTopOf="@+id/entryDate2"
                app:layout_constraintEnd_toEndOf="@+id/adress2"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/adress2"
                app:layout_constraintTop_toBottomOf="@+id/adress2"
                app:layout_constraintVertical_bias="0.0" />

            <EditText
                android:id="@+id/adress2"
                android:layout_width="350dp"
                android:layout_height="47dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/inpuut_bg"
                android:drawableLeft="@android:drawable/ic_dialog_map"
                android:ems="10"
                android:hint=" Address "
                android:inputType="textPostalAddress"
                android:padding="10dp"
                android:paddingLeft="20dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:textColorHint="#0C0C0C"
                app:layout_constraintEnd_toEndOf="@+id/textView3"
                app:layout_constraintHorizontal_bias="0.444"
                app:layout_constraintStart_toStartOf="@+id/textView3"
                app:layout_constraintTop_toBottomOf="@+id/textView3" />

            <EditText
                android:id="@+id/entryDate2"
                android:layout_width="359dp"
                android:layout_height="54dp"
                android:layout_marginTop="28dp"
                android:background="@drawable/inpuut_bg"
                android:drawableLeft="@android:drawable/ic_menu_month"
                android:ems="10"
                android:hint="Enter Entry date"
                android:inputType="date"
                android:padding="10dp"
                android:paddingLeft="20dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:textColorHint="#101010"
                app:layout_constraintBottom_toTopOf="@+id/exitDate2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="342dp"
                android:layout_height="27dp"
                android:text="     Student OutPass"
                android:textAllCaps="false"
                android:textColor="#3F51B5"
                android:textSize="34sp"
                android:textStyle="bold|italic"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ListView
                android:id="@+id/list"
                android:layout_width="412dp"
                android:layout_height="388dp"
                android:visibility="invisible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textView3">

                <EditText
                    android:id="@+id/nameinstudentutpass"
                    android:layout_width="316dp"
                    android:layout_height="42dp"
                    android:background="@drawable/inpuut_bg"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:text="Name" />
            </ListView>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/usernameparentsshow"
        android:layout_width="282dp"
        android:layout_height="40dp"
        android:text="Username"
        android:textColor="#070707"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.131"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.015" />

    <TextView
        android:id="@+id/showPhone"
        android:layout_width="278dp"
        android:layout_height="31dp"
        android:text="proffession"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/usernameparentsshow"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/usernameparentsshow"
        app:layout_constraintTop_toBottomOf="@+id/usernameparentsshow" />

    <Button
        android:id="@+id/request9810"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginLeft="56dp"
        android:text="request"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/cancelRequest"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView"
        app:layout_constraintVertical_bias="0.151" />

    <Button
        android:id="@+id/cancelRequest"
        android:layout_width="176dp"
        android:layout_height="48dp"
        android:text="cancel Request"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.931"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.781" />

    <TextView
        android:id="@+id/showEmail"
        android:layout_width="270dp"
        android:layout_height="26dp"
        android:layout_marginTop="1dp"
        android:text="ShowEmail"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/cardView"
        app:layout_constraintEnd_toEndOf="@+id/showPhone"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/showPhone"
        app:layout_constraintTop_toBottomOf="@+id/showPhone"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/newreq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="new request"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/pushreqtowarden"
        app:layout_constraintEnd_toStartOf="@+id/cancelRequest"
        app:layout_constraintHorizontal_bias="0.196"
        app:layout_constraintStart_toStartOf="@+id/pushreqtowarden"
        app:layout_constraintTop_toBottomOf="@+id/cardView"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="97dp"
        android:layout_height="91dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:background="@drawable/ic_face"
        android:hapticFeedbackEnabled="true"
        app:layout_constraintBottom_toBottomOf="@+id/showEmail"
        app:layout_constraintEnd_toEndOf="@+id/cardView"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toEndOf="@+id/showPhone"
        app:layout_constraintTop_toTopOf="@+id/usernameparentsshow"
        app:layout_constraintVertical_bias="1.0"
        tools:srcCompat="@drawable/ic_face" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="119dp"
        android:layout_height="133dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.883"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.973"
        app:srcCompat="@drawable/jgiicon" />

    <Button
        android:id="@+id/pushreqtowarden"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="156dp"
        android:layout_marginRight="156dp"
        android:text="requestToWarden"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/cancelRequest"
        app:layout_constraintHorizontal_bias="0.246"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/request9810"
        app:layout_constraintVertical_bias="0.427" />

</androidx.constraintlayout.widget.ConstraintLayout>